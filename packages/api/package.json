{
    "name": "@cennznet/api_test",
    "version": "0.9.4",
    "description": "cennznet's javascript client",
    "keywords": [
        "cennz"
    ],
    "author": "ian.he <ian.he@centrality.ai>",
    "homepage": "https://bitbucket.org/centralitydev/cennznet-js#readme",
    "license": "ISC",
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@bitbucket.org/centralitydev/cennznet-js.git"
    },
    "scripts": {
        "build": "rimraf dist && rollup -c rollup.config.ts",
        "test:unit": "node --inspect ./node_modules/.bin/jest -c ./jest.config.js spec",
        "test:e2e": "jest --silent --runInBand -c ./jest.config.js e2e",
        "test:e2eDebug": "node --inspect-brk ./node_modules/.bin/jest --silent --runInBand -c ./jest.config.js e2e",
        "doc": "npx compodoc --tsconfig ../../tsconfig.json --hideGenerator --theme readthedocs --includes compodoc --output ../../docs/api"
    },
    "dependencies": {
        "@cennznet/types": "^0.9.4",
        "@polkadot/api": "^0.48.0-beta.1",
        "@polkadot/extrinsics": "^0.48.0-beta.1",
        "@polkadot/rpc-core": "^0.48.0-beta.1",
        "@polkadot/rpc-provider": "^0.48.0-beta.1",
        "@polkadot/storage": "^0.48.0-beta.1",
        "@polkadot/types": "^0.48.0-beta.1",
        "eventemitter3": "^3.1.0"
    },
    "devDependencies": {
        "@types/jest": "^23.3.5",
        "jest": "24.1.0",
        "rimraf": "^2.6.2",
        "rollup": "^0.66.6",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-json": "^3.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript2": "^0.17.1",
        "ts-mockito": "^2.3.1",
        "typedoc": "^0.13.0",
        "typedoc-plugin-markdown": "^1.1.19",
        "typescript": "^3.1.6"
    }
}
